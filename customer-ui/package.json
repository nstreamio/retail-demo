{
  "name": "@swim/retail-demo",
  "version": "0.1.0",
  "description": "Swim Retail Demo",
  "homepage": "https://github.com/swimos/retail-demo",
  "copyright": "2015-2022 Swim.inc",
  "license": "Apache-2.0",
  "repository": "https://github.com/swimos/retail-demo.git",
  "main": "build/javascript/retail-demo.js",
  "devDependencies": {
    "@microsoft/api-extractor": "7.35",
    "@rollup/plugin-node-resolve": "15",
    "@rollup/plugin-terser": "0.4",
    "@types/eslint": "8",
    "@types/node": "20",
    "@typescript-eslint/eslint-plugin": "5",
    "@typescript-eslint/parser": "5",
    "eslint": "8",
    "rollup": "2.79",
    "rollup-plugin-sourcemaps": "0.6",
    "terser": "5",
    "tslib": ">=2.5",
    "typedoc": "0.25.1",
    "typescript": "~5.2",
    "rollup-plugin-terser": "7"
  },
  "dependencies": {
    "@nstream/atlas": "dev",
    "@nstream/domain": "dev",
    "@nstream/gui": "dev",
    "@nstream/nstream": "dev",
    "@nstream/session": "dev",
    "@nstream/shell": "dev",
    "@nstream/widget": "dev",
    "@swim/button": "dev",
    "@swim/chart": "dev",
    "@swim/client": "dev",
    "@swim/codec": "dev",
    "@swim/collections": "dev",
    "@swim/component": "dev",
    "@swim/constraint": "dev",
    "@swim/controller": "dev",
    "@swim/core": "dev",
    "@swim/dom": "dev",
    "@swim/gauge": "dev",
    "@swim/geo": "dev",
    "@swim/graphics": "dev",
    "@swim/host": "dev",
    "@swim/leaflet": "dev",
    "@swim/math": "dev",
    "@swim/maps": "dev",
    "@swim/model": "dev",
    "@swim/panel": "dev",
    "@swim/pie": "dev",
    "@swim/recon": "dev",
    "@swim/runtime": "dev",
    "@swim/sheet": "dev",
    "@swim/structure": "dev",
    "@swim/style": "dev",
    "@swim/table": "dev",
    "@swim/theme": "dev",
    "@swim/time": "dev",
    "@swim/token": "dev",
    "@swim/toolbar": "dev",
    "@swim/toolkit": "dev",
    "@swim/ui": "dev",
    "@swim/uri": "dev",
    "@swim/util": "dev",
    "@swim/ux": "dev",
    "@swim/view": "dev",
    "@swim/vis": "dev",
    "@swim/warp": "dev",
    "@swim/window": "dev"
  },
  "scripts": {
    "mkdir-ui": "mkdir -p dist/libs && mkdir -p dist/assets",
    "copy-core": "cp node_modules/@swim/core/dist/umd/*.min.js* dist/libs",
    "copy-host": "cp node_modules/@swim/host/dist/umd/*.min.js* dist/libs",
    "copy-ui": "cp node_modules/@swim/ui/dist/umd/*.min.js* dist/libs",
    "copy-ux": "cp node_modules/@swim/ux/dist/umd/*.min.js* dist/libs",
    "copy-vis": "cp node_modules/@swim/vis/dist/umd/*.min.js* dist/libs",
    "copy-maps": "cp node_modules/@swim/maps/dist/umd/*.min.js* dist/libs",
    "copy-gui": "cp node_modules/@nstream/gui/dist/umd/*.min.js* dist/libs",
    "copy-libs": "npm run copy-core && npm run copy-host && npm run copy-ui && npm run copy-ux && npm run copy-vis && npm run copy-maps && npm run copy-gui",
    "compile": "tsc -b",
    "bundle": "cd src/ && rollup -c",
    "copy-app": "cp index.html dist/ && cp build/javascript/*.min.js* dist/libs && cp assets/* dist/assets",
    "build": "npm run compile && npm run bundle && npm run copy-app",
    "postinstall": "npm run mkdir-ui && npm run copy-libs"
  }
}
